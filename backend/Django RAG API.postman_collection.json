								"exec": [
									"pm.test(\"Status code is 201\", function () {\r",
									"    pm.expect(pm.response.code).to.equal(201);\r",
									"});\r",
									"\r",
									"// Parse the response body\r",
									"const response = pm.response.json();\r",
									"\r",
									"// Save tokens to environment variables\r",
									"pm.environment.set(\"refresh-token\", response[\"refresh-token\"]);\r",
									"pm.environment.set(\"access-token\", response[\"access-token\"]);\r",
									"\r",
									"// Additional validation for tokens\r",
									"pm.test(\"Refresh token exists\", function () {\r",
									"    pm.expect(response[\"refresh-token\"]).to.exist;\r",
									"    pm.expect(response[\"refresh-token\"]).to.be.a('string');\r",
									"});\r",
